// react ç”¨
{
  "compilerOptions": {
    "jsx": "react",
    "target": "es5",
    "module": "commonjs",
    "sourceMap": false,
    "baseUrl": "./",
    "outDir": "./dist",
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "downlevelIteration":true,

    "allowJs": true,
    "declaration": false,

    "noImplicitAny": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "noEmitOnError": true
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}